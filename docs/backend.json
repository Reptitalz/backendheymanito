{
  "entities": {
    "User": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "User",
      "type": "object",
      "description": "Represents a user of the Hey Manito! application.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the user."
        },
        "firebaseUid": {
          "type": "string",
          "description": "The Firebase UID associated with the user's authentication."
        },
        "email": {
          "type": "string",
          "description": "The user's email address.",
          "format": "email"
        },
        "name": {
          "type": "string",
          "description": "The user's name."
        },
        "createdAt": {
          "type": "string",
          "description": "The date and time the user account was created.",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "firebaseUid",
        "email",
        "name",
        "createdAt"
      ]
    },
    "Plan": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Plan",
      "type": "object",
      "description": "Represents a subscription plan for the Hey Manito! application.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the plan."
        },
        "name": {
          "type": "string",
          "description": "The name of the plan (e.g., Basic, Premium)."
        },
        "credits": {
          "type": "number",
          "description": "The number of credits included in the plan. 1 credit = 1000 messages."
        },
        "price": {
          "type": "number",
          "description": "The price of the plan."
        },
        "description": {
          "type": "string",
          "description": "A description of the plan's features and benefits."
        }
      },
      "required": [
        "id",
        "name",
        "credits",
        "price",
        "description"
      ]
    },
    "Subscription": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Subscription",
      "type": "object",
      "description": "Represents a user's subscription to a specific plan.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the subscription."
        },
        "userId": {
          "type": "string",
          "description": "Reference to User. (Relationship: User 1:N Subscription)"
        },
        "planId": {
          "type": "string",
          "description": "Reference to Plan. (Relationship: Plan 1:N Subscription)"
        },
        "startDate": {
          "type": "string",
          "description": "The date the subscription started.",
          "format": "date-time"
        },
        "endDate": {
          "type": "string",
          "description": "The date the subscription ends.",
          "format": "date-time"
        },
        "status": {
          "type": "string",
          "description": "The status of the subscription (e.g., active, cancelled)."
        }
      },
      "required": [
        "id",
        "userId",
        "planId",
        "startDate",
        "endDate",
        "status"
      ]
    },
    "Assistant": {
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Assistant",
      "type": "object",
      "description": "Represents a WhatsApp bot assistant managed within the Hey Manito! application.",
      "properties": {
        "id": {
          "type": "string",
          "description": "Unique identifier for the assistant."
        },
        "userId": {
          "type": "string",
          "description": "Reference to User. (Relationship: User 1:N Assistant)"
        },
        "name": {
          "type": "string",
          "description": "The name of the assistant."
        },
        "description": {
          "type": "string",
          "description": "A description of the assistant's purpose."
        },
        "phoneNumber": {
          "type": "string",
          "description": "The phone number associated with the WhatsApp bot."
        },
        "createdAt": {
          "type": "string",
          "description": "The date and time the assistant was created.",
          "format": "date-time"
        }
      },
      "required": [
        "id",
        "userId",
        "name",
        "description",
        "phoneNumber",
        "createdAt"
      ]
    }
  },
  "auth": {
    "providers": [
      "google.com"
    ]
  }
}
